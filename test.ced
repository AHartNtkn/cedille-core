$the
  #T *
  #x T
  x

$CNat
  @P *
  @Z P
  @S @ P P
  P

$CZ
  #P *
  #Z P
  #S @ P P
  Z

$CS
  #n CNat
  #P *
  #Z P
  #S @ P P
  /S ///n P Z S

$CAdd
  #n CNat
  #m CNat
  #P *
  #Z P
  #S @ P P
  ///n P
    ///m P Z S
    S

$C0
  CZ

$C1
  /CS C0

$C2
  /CS C1

$C3
  /CS C2

$bad
  #T *
  /T T

$C1_eq_C1
  =C1 C1

$C2_eq_C2
  =C2 C2

$C1_refl
  :C1 the

$CBool
  &P *
  @T P
  @F P
  P

$CTrue
  %P *
  #T P
  #F P
  T

$CFalse
  %P *
  #T P
  #F P
  F

$IBool
  #x CBool
  @P @ CBool *
  @T /P CTrue
  @F /P CFalse
  /P x

$Bool
  ^b CBool /IBool b

Bool
